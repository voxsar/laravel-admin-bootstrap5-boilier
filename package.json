        "lodash": "^4.17.19",
        "material-design-icons-iconfont": "^6.1.0",
        "postcss-import": "^12.0.1",
        "postcss-rtl": "^1.7.3",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.28.0",
        "sass-loader": "^10.0.5",
        "tailwindcss": "^1.8.0",
        "vue-template-compiler": "^2.6.12",
        "webpack-chunk-rename-plugin": "^1.0.3"
    }
